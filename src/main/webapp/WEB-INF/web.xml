<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="2.5"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>check_login</servlet-name>
    <servlet-class>login_servlets.Check_login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>check_login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>for_to_regist</servlet-name>
    <servlet-class>regis_servlet.For_to_regis</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>for_to_regist</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>check_regis</servlet-name>
    <servlet-class>regis_servlet.Check_registration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>check_regis</servlet-name>
    <url-pattern>/registration_done</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>delete_user</servlet-name>
    <servlet-class>delete_servlet.Delete_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delete_user</servlet-name>
    <url-pattern>/delete_servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>add_user</servlet-name>
    <servlet-class>add_servlet.Add_user</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>add_user</servlet-name>
    <url-pattern>/add_user</url-pattern>
  </servlet-mapping>

</web-app>

